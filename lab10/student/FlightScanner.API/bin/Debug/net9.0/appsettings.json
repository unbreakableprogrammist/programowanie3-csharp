{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AirlinesConfig": {
    "Providers": [
      {
        "Id": "reliable-air",
        "Name": "Reliable Air",
        "Endpoint": "/api/flights/reliable-air",
        "Configuration": {
          "MinDelayMs": 800,
          "MaxDelayMs": 1500,
          "Probabilities": {
            "NotFound": 0.0,
            "Unauthorized": 0.0,
            "ServerError": 0.05
          }
        }
      },
      {
        "Id": "budget-wings",
        "Name": "Budget Wings",
        "Endpoint": "/api/flights/budget-wings",
        "Configuration": {
          "MinDelayMs": 200,
          "MaxDelayMs": 700,
          "Probabilities": {
            "NotFound": 0.1,
            "Unauthorized": 0.0,
            "ServerError": 0.15
          }
        }
      },
      {
        "Id": "slow-sky",
        "Name": "Slow Sky",
        "Endpoint": "/api/flights/slow-sky",
        "Configuration": {
          "MinDelayMs": 3500,
          "MaxDelayMs": 5000,
          "Probabilities": {
            "NotFound": 0.0,
            "Unauthorized": 0.0,
            "ServerError": 0.0
          }
        }
      },
      {
        "Id": "phantom-flights",
        "Name": "Phantom Flights",
        "Endpoint": "/api/flights/phantom-flights",
        "Configuration": {
          "MinDelayMs": 500,
          "MaxDelayMs": 1200,
          "Probabilities": {
            "NotFound": 0.4,
            "Unauthorized": 0.0,
            "ServerError": 0.0
          }
        }
      },
      {
        "Id": "secure-jet",
        "Name": "Secure Jet",
        "Endpoint": "/api/flights/secure-jet",
        "Configuration": {
          "MinDelayMs": 700,
          "MaxDelayMs": 1300,
          "Probabilities": {
            "NotFound": 0.0,
            "Unauthorized": 0.25,
            "ServerError": 0.0
          }
        }
      }
    ]
  }
}
